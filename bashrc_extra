# .bash_userspec

# Aliases
alias ll='ls -lh'
alias la='ls -lah'

# 
export PATH="$HOME/.rbenv/bin:$PATH"
export PATH="$HOME/.rbenv/plugins/ruby-build/bin:$PATH"
eval "$(rbenv init -)"


#git prompt
source ~/.git_prompt.sh
